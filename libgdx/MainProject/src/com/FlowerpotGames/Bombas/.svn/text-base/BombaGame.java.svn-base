package com.FlowerpotGames.Bombas;

import com.badlogic.gdx.Game;

public class BombaGame extends Game
{
	public final GameScreen mGameScene;
	
	public final static int SCREEN_WIDTH = 614;
	public final static int SCREEN_HEIGHT = 1024;
	
	public BombaGame()
	{
		mGameScene = new GameScreen(this);
	}
	
    @Override
    public void create()
    {
    	this.setScreen(mGameScene);
    }
}
