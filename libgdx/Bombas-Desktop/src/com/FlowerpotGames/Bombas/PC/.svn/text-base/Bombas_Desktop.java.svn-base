package com.FlowerpotGames.Bombas.PC;

import org.lwjgl.opengl.Display;

import com.FlowerpotGames.Bombas.BombaGame;
import com.badlogic.gdx.backends.lwjgl.LwjglApplication;

public class Bombas_Desktop
{
	public static void main(String[] args)
	{
		new LwjglApplication(new BombaGame(), "Game", BombaGame.SCREEN_WIDTH/2, BombaGame.SCREEN_HEIGHT/2, false);
		Display.setVSyncEnabled(true);
	}
}
